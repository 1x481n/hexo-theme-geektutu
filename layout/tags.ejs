---
layout: layout
---
<!-- Main Content -->
<div class="col-xs-12">
    <div class="post-list">

        <!-- 标签列表 -->
        <ul>
            <% site.tags.forEach(function(tag, index){  %>
            <li class="post-item-parent post-item-color-<%- index % 5 %>">
                <a href="<%- config.root %>tags#<%- tag.name %>" class="post-text-parent" id="<%= tag.name %>">
                    <%- tag.name %>
                </a>
            </li>
            <% tag.posts.map(function(post){  %>
            <li>
                <a class="post-item-title" href="<%- config.root %><%- post.path %>"><%- post.title || "Untitled" %></a>
            </li>
            <% }) %>
            <% }) %>
        </ul>

    </div>
</div>